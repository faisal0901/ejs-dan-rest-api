 <div
          class="modal fade"
          id="editModal-activity"
          tabindex="-1"
          role="dialog"
          aria-labelledby="addModalLabel"
          aria-hidden="true"
        >
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="addModalLabel">Add Bank</h5>
                <button
                  type="button"
                  class="close"
                  data-dismiss="modal"
                  aria-label="Close"
                >
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <form method="post" action="/admin/Item/view-detail-item/edit/activity/?_method=PUT" enctype="multipart/form-data"> 
              <div class="modal-body">
                  <div class="form-group">
                    <label for="name">name </label>
                    <input type="text" class="form-control name"  name="name" placeholder="name" required> 
                  </div>
                  <div class="form-group">
                    <label for="type">type</label>
                    <input type="text" class="form-control type"  name="type" placeholder="type" required> 
                  </div>
                  <div class="form-group">
                    <label for="name">Image</label>
                    <input type="file" class="form-control image"  name="imageUrl" placeholder="upload image" > 
                    <span>pilih gambar untuk update</span>
                  </div>
                </div>
                <div class="modal-footer">
                  <button
                  type="button"
                  class="btn btn-secondary"
                  data-dismiss="modal"
                  >
                  Close
                </button>
               <input type="hidden" value="<%= itemId %>" name="itemId" />
                <input type="hidden" name="id" class="id">
                <button type="submit" class="btn btn-primary ">
                  Edit
                </button>
              </form>
              </div>
            </div>
          </div>
        </div>
      </div>